{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "webapp/*": ["../../app/webapp/*"],
      "src/*": ["../../app/webapp/src/*"],
      "shared/*": ["../../shared/*"],
      "components/*": ["../../shared/components/src/*"],
      "images/*": ["../../shared/assets/images/*"]
    }
  },
  "exclude": ["node_modules"],
  "path-intellisense.autoSlashAfterDirectory": true
}
